      <main class="main">
        <div class="container mt-5">
          <div class="d-flex justify-content-center my-4 h3">
            <form action="/admin/salesreport" method="get">
              <label for="startDate" class="h4 mx-3">Start Date:</label>
              <input type="date" id="startDate" name="startDate" />
              <label for="endDate" class="h4 mx-3">End Date:</label>
              <input
                type="date"
                id="endDate"
                name="endDate"
              />
              <button type="submit" class="btn btn-primary mx-2">
                <h4>Filter</h4>
              </button>
              <span id="dateError"></span>
            </form>
          </div>
          <div class="table-responsive" id="no-more-tables">
            <table class="table table-striped bg-white" id="myTable">
              <thead class="bg-dark text-light">
                <tr>
                  <th
                    style="font-size: 20px; text-align: center"
                    scope="col"
                    class="sorting"
                  >
                    Date
                  </th>
                  <th
                    style="font-size: 20px; text-align: center"
                    scope="col"
                    class="sorting"
                  >
                    Order Id
                  </th>
                  <th
                    style="font-size: 20px; text-align: center"
                    scope="col"
                    class="sorting"
                  >
                    Customer
                  </th>
                  <th
                    style="font-size: 20px; text-align: center"
                    scope="col"
                    class="sorting"
                  >
                    Address
                  </th>
                  <th
                    style="font-size: 20px; text-align: center"
                    scope="col"
                    class="sorting"
                  >
                    Products
                  </th>
                  <th
                    style="font-size: 20px; text-align: center"
                    scope="col"
                    class="sorting"
                  >
                    Total Amount
                  </th>
                  <th
                    style="font-size: 20px; text-align: center"
                    scope="col"
                    class="sorting"
                  >
                    Payment
                  </th>
                </tr>
              </thead>
              <tbody>
                <tr style="font-size: medium; text-align: center">
                  <td data-title="Date"></td>
                  <td data-title="OrderId"></td>
                  <td data-title="Customer"></td>
                  <td data-title="Address"></td>
                 
                  <td data-title="Products"></td>
                  <td data-title="TotalPrice"></td>

                  <td data-title="Payment"></td>
                  <td data-title="Payment">Online</td>
                </tr>
                <tr>
                  <td colspan="5">Orders Not Found</td>
                </tr>
              </tbody>
            </table>
          </div>
          <div class="d-flex justify-content-around my-5">
            <h3>Total : Rs.</h3>
          </div>
          <div class="d-flex justify-content-evenly mx-3">
            <button class="btn-danger" >
              <h3>Download Excel</h3>
            </button>
            <button class="btn-danger" >
              <h3>Download PDF</h3>
            </button>
