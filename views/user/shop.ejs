                                    <% if (products.length != 0) { %>                                    

                                    <% for (let i = 0; i < products.length; i++) { %>
                                        <% if (products[i].is_blocked==false) { %>
                                            <div class="col-6 col-md-4 col-lg-4">
                                                <div class="product product-7 text-center">
        
                                                    <figure class="product-media">
                                                        
                                                        <a href="/singlepage?id=<%= products[i]._id %>">
                                                            <img src="<%= products[i].images[0] %>" alt="Product image" class="product-image">
                                                        </a>
        
                                                        <div class="product-action-vertical">
                                                            <a onclick="handleAddToWishlist('<%=products[i]._id %>')" class="btn-product-icon btn-wishlist btn-expandable"><span>add to wishlist</span></a>
                                                            <a href="popup/quickView.html" class="btn-product-icon btn-quickview" title="Quick view"><span>Quick view</span></a>
                                                        </div><!-- End .product-action-vertical -->

                                                        
                                                            <div class="product-action">
                                                                <a  onclick="handleAddToCart('<%=products[i]._id %>')" style="cursor: pointer;" class="btn-product btn-cart"><span>add to cart</span></a>

                                                            </div><!-- End .product-action -->


                                                    </figure><!-- End .product-media -->
        
                                                    <div class="product-body">
                                                        <div class="product-cat">
                                                            <a href="#"></a>
                                                        </div><!-- End .product-cat -->
                                                        <h3 class="product-title"><a href="product.html"><%=products[i].productName%></a></h3><!-- End .product-title -->
                                                        <div class="product-price">
                                                            ₹<%=products[i].productPrice%>
                                                        </div><!-- End .product-price -->
                                                        <div class="ratings-container">
                                                            <div class="ratings">
                                                                <% let averageRating=0 %>
                                                                <% if (products[i].reviews.length!=0) { %>
                                                                    <%
                                                                    let sumOfRatings = 0;
                        
                                                                sumOfRatings = products[i].reviews.reduce((sum, r) => sum + Number(r.rating), 0);
                        
                                                                const numberOfRatings = products[i].reviews.length;
                        
                                                                 averageRating = sumOfRatings / numberOfRatings;
                        
                        
                                                           %>    
                                                                <% } %>
                                                
                                                                
                                                                <div class="ratings-val" style="width: <%=averageRating%>%;"></div><!-- End .ratings-val -->
                                                            </div><!-- End .ratings -->
                                                            <span class="ratings-text">( <%=products[i].reviews.length %> Reviews )</span>
                                                        </div><!-- End .rating-container -->
        
                                                        <div class="product-nav product-nav-thumbs">
                                                            <% for( let j = 0; j < products[i].images.length; j++ ) { %>
                                                                <a href="#" class="active">
                                                                    <img src="<%= products[i].images[j] %>" alt="product desc">
                                                                </a>
                                                            <% } %>
                                                           
                                                            
                                                        </div><!-- End .product-nav -->
                                                    </div><!-- End .product-body -->
                                                </div><!-- End .product -->
                                            </div><!-- End .col-sm-6 col-lg-4 -->
                                         
                                        <% } %>

                                       
                                        <% } %>

                                

        

                                   

