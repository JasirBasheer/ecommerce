    function confirmCancelOrder(productId,productPrice) {
  handleAlert().then((confirmed) => {
    if (confirmed) {
      handlecancelOrder(productId,productPrice);
    }
  });
}


function handlecancelOrder(productId,productPrice) {
    
  fetch('/cancelorder', {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json'
    },
    body: JSON.stringify({ productId,productPrice })
  })
  .then(response => {

            
if (response.redirected) {

    window.location.href = response.url;
}else{
    return response.json()
}
})  .then(data => {
    
    if (data.success) {
      Swal.fire({
        title: "Cancelled!",
        text: "Your order has been cancelled.",
        icon: "success"
      });

    } else {
      Swal.fire({
        title: "Error!",
        text: "Failed to cancel the order.",
        icon: "error"
      });
    }

    window.location.reload();
  })
  .catch(error => {
    console.error('Error:', error);
    Swal.fire({
      title: "Error!",
      text: "An unexpected error occurred.",
      icon: "error"
    });
  });
}

